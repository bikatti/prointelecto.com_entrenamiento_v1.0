.o-navContainer
 .o-containerNavInner.o-containerNavWhite.o-containerNavBlue
  .m-revealInner
  //- Aqu√≠ comienza el header del nav
  .m-containerHeading
   span.m-containerHeading__itemHeader.-linkHover
    +menuSectionHeader()
   a.m-containerHeading__brand.d-none(href="/dist/")
    .m-containerHeading__img
  //- Links a los contenidos
  .m-scroll__containerNavChildren.-globalMenu.-contentMenu
   .m-scroll__scrollContainer.-globalMenu.-contentMenu
    .m-scroll__content.d-none
     div(aria-label="otro" role="group")
      +itemContent("","#","Como estas")
     div(aria-label="Cosas Nuevas" role="group")
      +titleGroupContent("","cosas nuevas")
      +itemContent(" -modifierLink","/odoo/page-prueba.html","primer links")
    //- Menu global
    .m-scrollGlobal__content
     a(class="a-itemContent -globalLink -colorWhite", href="/dist/")
      .a-itemPart
       .a-itemPart__content Hello
     div(aria-label="Cosas Nuevas" role="group")
      +titleGroupContent("-globalMenu","cosas nuevas")
      +linkPageMenuGlobal()
      +linkPageMenuGlobal("/dist/odoo/page-prueba.html", "Odoo page")
  //- Algo
  .o-globalNavSec_container
   .m-revealInner